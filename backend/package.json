    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "express-rate-limit": "^5.5.1",
    "helmet": "^4.6.0",
    "http-errors": "~1.6.3",
    "http-status": "^1.5.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment": "^2.29.4",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.4.1",
